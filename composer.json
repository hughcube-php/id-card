{
    "name": "hughcube/id-card",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "hugh.li",
            "email": "hugh.li@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "nesbot/carbon": "*",
        "hughcube/enum": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "*",
        "guzzlehttp/guzzle": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "HughCube\\IdCard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HughCube\\IdCard\\Tests\\": "tests"
        }
    },
    "extra": {},
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse -vvv --memory-limit=-1 src/",
        "check-style": "phpcs -p --standard=PSR2 src/ -v",
        "fix-style": "phpcbf -p --standard=PSR2 src/ -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
